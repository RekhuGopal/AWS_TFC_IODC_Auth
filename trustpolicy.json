{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Effect": "Allow",
            "Principal": {
                "Federated": "arn:aws:iam::357171621133:oidc-provider/app.terraform.io"
            },
            "Action": "sts:AssumeRoleWithWebIdentity",
            "Condition": {
                "StringEquals": {
                    "SITE_ADDRESS:aud": "aws.workload.identity",
                    "SITE_ADDRESS:sub": "organization:CloudQuickLabs:project:Test-Project:workspace:TFC-AWS-OIDC:run_phase:*"
                }
            }
        }
    ]
}
